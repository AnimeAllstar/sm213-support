{
    "name": "sm213-syntax-highlighter",
    "displayName": "SM213 Syntax Highlighter",
    "version": "1.0.0",
    "publisher": "AsadDhorajiwala",
    "description": "Syntax highlighting for SM213",
    "author": {
        "name": "Asad Dhorajiwala"
      },
    "engines": {
        "vscode": "^1.53.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "sm213",
            "aliases": ["SM213", "sm213"],
            "extensions": [".s"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "sm213",
            "scopeName": "source.s",
            "path": "./syntaxes/sm213.tmLanguage.json"
        }]
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/AnimeAllstar/sm213-syntax-highlighter"
      },
    "homepage": "https://github.com/AnimeAllstar/sm213-syntax-highlighter/blob/master/README.md"
}